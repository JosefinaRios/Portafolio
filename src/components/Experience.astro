---
import ExperienceItem from "./ExperienceItem.astro"

const EXPERIENE=[
    {
      date:"Diciembre 2024",
      title: "Practica Profesional",
      description: "Desarrollo un sistema web para la gestión de un taller de chapa y pintura, utilizando ASP.NET Core MVC, C# y SQL Server, enfocado en la administración de clientes, vehículos, etc.",
      
    }
]
---
<ol class="relative border-s border-gray-200 ml-3">
    {
        EXPERIENE.map(experience =>(
        <li class="mb-10 ms-4">
        <ExperienceItem {... experience}></ExperienceItem>
        </li>
        ))
    }                  
   
</ol>

